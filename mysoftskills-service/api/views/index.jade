extends index_layout
block content
    //div(class="parallax-container")
    //    div(class="parallax")
    //        img(src="http://lorempixel.com/580/250/nature/1")
    div(class="section white")
        div(class="row container")
            div(class="row col s12 card-panel grey lighten-5 ").panel-heading.center
                h1 MY SOFT SKILLS
                div(class="slider").panel-body
                    ul(class="slides")
                        li
                            img(src="http://lorempixel.com/580/250/nature/1")
                            div(class="caption center-align")
                                h3 FIRST IMAGE
                                h5(class="light grey-text text-lighten-3") THI IS TEST
                        li
                            img(src="http://lorempixel.com/580/250/nature/2")
                            div(class="caption left-align")
                                h3 SECOND IMAGE
                                h5(class="light grey-text text-lighten-3") THI IS TEST 2
                        li
                            img(src="http://lorempixel.com/580/250/nature/3")
                            div(class="caption right-align")
                                h3 THIRD IMAGE
                                h5(class="light grey-text text-lighten-3") THI IS TEST 3
                        li
                            img(src="http://lorempixel.com/580/250/nature/4")
                            div(class="caption center-align")
                                h3 FOURTH IMAGE
                                h5(class="light grey-text text-lighten-3") THI IS TEST 4
                //div(class="carousel")
                //    a(class="carousel-item" href="#one!")
                //        img(src="http://lorempixel.com/250/250/nature/1")
                //    a(class="carousel-item" href="#one!")
                //        img(src="http://lorempixel.com/250/250/nature/2")
                //    a(class="carousel-item" href="#one!")
                //        img(src="http://lorempixel.com/250/250/nature/3")
                //    a(class="carousel-item" href="#one!")
                //        img(src="http://lorempixel.com/250/250/nature/4")
                //    a(class="carousel-item" href="#one!")
                //        img(src="http://lorempixel.com/250/250/nature/5")
                div(class="row col s4").center
                    form(method="get" action="/signin" enctype="application/x-www-form-urlencoded")
                        div.panel-footer
                            input(type="submit" class="btn waves-effect waves-light" value="Login")
                div(class="row col s4").center
                    form(method="get" action="/signup" enctype="application/x-www-form-urlencoded")
                        div.panel-footer
                            input(type="submit" class="btn waves-effect waves-light" value="Registro")
                div(class="row col s4").center
                    a(href="#!" class="btn waves-effect waves-teal") FACEBOOK
